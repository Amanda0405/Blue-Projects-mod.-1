if(q2 == "Sim")

{soma = soma +1} 

else{soma = soma + 0}

let q3 = prompt("Mora perto da vítima?");

if(q3 == "Sim")

{soma = soma +1} 

else {soma = soma + 0}

let q4 = prompt("Devia para a vítima?");

if(q4 == "Sim")

{soma = soma + 1}

else{soma = soma + 0}

let q5 = prompt("Já trabalhou com a vítima?");

if(q5 == "Sim")
 
{soma = soma + 1} 

else{soma = soma + 0}

if(soma < 2 )
{console.log("Você é inocente.")}

else if(soma == 2)
{console.log("Você é considerada Suspeita.")}

 else if(soma >= 3 && soma <= 4)
{console.log("Você foi Cúmplice do crime.")}

else if(soma == 5 )
console.log("Voce é assassina")
